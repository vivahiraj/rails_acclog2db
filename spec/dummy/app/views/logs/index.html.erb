<h1>Listing logs</h1>

<table>
  <tr>
    <th>ID</th>
    <th>login</th>
    <th>ip</th>
    <th>url</th>
    <th>param</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.id %></td>
    <td><%= log.login %></td>
    <td><%= log.ip %></td>
    <td><%= log.url %></td>
    <td><%= log.param %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
